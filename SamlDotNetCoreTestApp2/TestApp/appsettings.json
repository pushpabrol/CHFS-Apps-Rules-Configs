            "PartnerCertificates": [
              {
                "FileName": "certificates/chfs-samples.pem"
              }
            ]
          }
        ]
      }
    ],
    "partnerName": "urn:chfs-samples.us.auth0.com",
    "customLogoutUrl": "https://localhost:3000?returnTo="
